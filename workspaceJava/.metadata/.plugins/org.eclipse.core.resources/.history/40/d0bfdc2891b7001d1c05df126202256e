package basic01;

public class For04 {
	
	public static void main(String[] args) {
		
		for(int i=1; i<=5; i++){ // 1부터 5까지 1씩 증가
			for(int j=1; j<=3; j++) { // 1부터 3까지 1씩 증가
				// System.out.println(j); // 123123123123123
				// System.out.print(i); // 111222333444555
				 System.out.print("*"); //
			}
		}
	}
}
